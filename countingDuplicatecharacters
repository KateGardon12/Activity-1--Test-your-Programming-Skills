package KateJenniferC.Gardon;

public class countingDuplicateharacters {

  public static void main(String[] args) {


System.out.println("How many duplicates are there");
String words = "Cherry Ann";
System.out.println(words);
//System.out.println(words.charAt(16))
String characters = "";
String duplicates = "";
for (int i = 0; i < words.length(); i++) {
 // System.out.println(words.charAt(i));
  String current = Character.toString(words.charAt(i));
  if(characters.contains(current)) {
  if(!duplicates.contains(current)){
     duplicates += current + ","; 
      }
 }
 
 characters += current;
 
 
 
 
}System.out.println(duplicates); 
 


  }

}



 

